# priklad http://www.cs.odu.edu/~toida/nerzic/390teched/regular/fa/nfa-2-dfa.html
( {s0, s1, s2, s3, s4}, 
  {'a', 'b'}, 
  {
    s0 'a' -> s1, 
    s0 'a' -> s2, 
    s0 'b' -> s2,
    s0 'a' -> s3,
    s0 'b' -> s3,
    s1 'a' -> s1,
    s1 'a' -> s2,
    s1 'b' -> s2,
    s1 'b' -> s3,
    s2 'b' -> s2,
    s2 'b' -> s4,
    s2 'b' -> s3,    
    s3 'b' -> s2,
    s3 'b' -> s3,
    s3 'a' -> s4,
    s3 'b' -> s4
  },
  s0, 
  {s1}
)
# Ocekavany vystup:
# s0 'a' -> s1_s2_s3
# s0 'b' -> s2_s3
# s1_s2 'a' -> s1_s2 
# s1_s2 'b' -> s2_s3_s4
# s1_s2_s3 'a' -> s1_s2_s4
# s1_s2_s3 'b' -> s2_s3_s4
# s1_s2_s4 'a' -> s1_s2
# s1_s2_s4 'b' -> s2_s3_s4
# s2_s3 'a' -> s4
# s2_s3 'b' -> s2_s3_s4
# s2_s3_s4 'a' -> s4
# s2_s3_s4 'b' -> s2_s3_s4
